    <script>
      
        function setCookie(cname, cvalue, exdays) {
          const d = new Date();
          d.setTime(d.getTime() + (exdays*24*60*60*1000));
          let expires = "expires="+ d.toUTCString();
          document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        }

        function getCookie(cname) {
          let name = cname + "=";
          let ca = document.cookie.split(';');
          for(let i = 0; i < ca.length; i++) {
            let c = ca[i];
            while (c.charAt(0) == ' ') {
              c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
              return c.substring(name.length, c.length);
            }
          }
          return "";
        }

        window.addEventListener('DOMContentLoaded', (event) => {
          if( getCookie( "preloader" ) != "" ) {
            var preloader = document.getElementById( "mfr-preloader" );
            preloader.remove();
          }
        });
      

      function preloaderInit() {
          
            if( getCookie( "preloader" ) != "" ) return;
          

          var preloader = document.getElementById( "mfr-preloader" );
          preloader.classList.add( "animate__animated", "animate__fadeOut" );

          preloader.addEventListener('animationend', () => {
              var preloaderCookieID = new Date().getTime();
              preloader.remove();

              
                  setCookie( "preloader", preloaderCookieID, 1 ); //set cookie for preloader for 1day.
              
          });
      }

      window.addEventListener('load', preloaderInit );
      document.addEventListener("shopify:section:load", preloaderInit );
  </script>




